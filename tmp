IMAGE_CMD_btrfs() {
	size=max
	if [ ${size} -lt 16384 ] ; then
		size=16384
		bbwarn "Rootfs size is too small for BTRFS. Filesystem will be extended to ${size}K"
	fi
	dd if=/dev/zero of=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.btrfs seek=${size} count=0 bs=1024
	mkfs.btrfs  -r /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.btrfs
}

IMAGE_CMD_cpio() {
	(cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs && find . | sort | cpio --reproducible -o -H newc >/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.cpio)
	# We only need the /init symlink if we're building the real
	# image. The -dbg image doesn't need it! By being clever
	# about this we also avoid 'touch' below failing, as it
	# might be trying to touch /sbin/init on the host since both
	# the normal and the -dbg image share the same WORKDIR
	if [ "${IMAGE_BUILDING_DEBUGFS}" != "true" ]; then
		if [ ! -L /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/init ] && [ ! -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/init ]; then
			if [ -L /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init ] || [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init ]; then
				ln -sf /sbin/init /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/cpio_append/init
			else
				touch /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/cpio_append/init
			fi
			(cd  /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/cpio_append && echo ./init | cpio -oA -H newc -F /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.cpio)
		fi
	fi
}

IMAGE_CMD_f2fs() {
        # We need to add additional smarts here form devices smaller than 1.5G
        # Need to scale appropriately between 40M -> 1.5G as the "overprovision
        # ratio" goes down as the device gets bigger (70% -> 4.5%), below about
        # 500M the standard IMAGE_OVERHEAD_FACTOR does not work, so add additional
        # space here when under 500M
	size=max
	if [ ${size} -lt 524288 ] ; then
		size=524288
		bbwarn "Rootfs size is too small for F2FS. Filesystem will be extended to ${size}K"
	fi
	dd if=/dev/zero of=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.f2fs seek=${size} count=0 bs=1024
	mkfs.f2fs  /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.f2fs
	sload.f2fs -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.f2fs
}

IMAGE_CMD_multiubi() {
	# Split MKUBIFS_ARGS_<name> and UBINIZE_ARGS_<name>
	for name in ${MULTIUBI_BUILD}; do
		eval local mkubifs_args=\"\$MKUBIFS_ARGS_${name}\"
		eval local ubinize_args=\"\$UBINIZE_ARGS_${name}\"

		multiubi_mkfs "${mkubifs_args}" "${ubinize_args}" "${name}"
	done
}

IMAGE_CMD_ubi() {
	multiubi_mkfs "${MKUBIFS_ARGS}" "${UBINIZE_ARGS}"
}

