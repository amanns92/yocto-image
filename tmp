# $base_bindir [4 operations]
#   exported /home/parallels/yocto/poky/meta/conf/bitbake.conf:24
#     [export] "1"
#   set /home/parallels/yocto/poky/meta/conf/bitbake.conf:24
#     "${root_prefix}/bin"
