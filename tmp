do_deploy_source_date_epoch() {
    mkdir -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-source-date-epoch
    if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/source-date-epoch/__source_date_epoch.txt ]; then
        echo "Deploying SDE from /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/source-date-epoch/__source_date_epoch.txt -> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-source-date-epoch."
        cp -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/source-date-epoch/__source_date_epoch.txt /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-source-date-epoch/__source_date_epoch.txt
    else
        echo "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/source-date-epoch/__source_date_epoch.txt not found!"
    fi
}

do_flush_pseudodb() {
	PSEUDO_PREFIX=/home/parallels/yocto/build/tmp/sysroots-components/aarch64/pseudo-native/usr PSEUDO_LOCALSTATEDIR=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/pseudo/ PSEUDO_PASSWD=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs:/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot-native PSEUDO_NOSYMLINKEXP=1 PSEUDO_IGNORE_PATHS=/usr/,/etc/,/usr/lib,/dev/,/run/,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot,/home/parallels/yocto/build/sstate-cache,/home/parallels/yocto/build/tmp/stamps,/home/parallels/yocto/build/tmp/sstate-control,/home/parallels/yocto/build/tmp/buildstats,/home/parallels/yocto/build/tmp/sysroots-components,/home/parallels/yocto/build/tmp/pkgdata,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sstate-build-package_,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sstate-install-package_,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/pkgdata-sysroot,/home/parallels/yocto/build/tmp/deploy,/home/parallels/yocto/build/cache,/home/parallels/yocto/poky/scripts,/home/parallels/yocto/poky/meta/files/common-licenses,/home/parallels/yocto/meta-raspberrypi/files/custom-licenses,/home/parallels/yocto/meta-openembedded/meta-oe/licenses,/home/parallels/yocto/meta-openembedded/meta-python/licenses,/home/parallels/yocto/meta-openembedded/meta-networking/licenses,/home/parallels/yocto/meta-openembedded/meta-multimedia/licenses,/home/parallels/yocto/poky/meta/COPYING,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/oe-sdk-repo,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sstate-build-populate_sdk,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/build-wic,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/intercept_scripts,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/oe-rootfs-repo,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sstate-build-image_complete,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/core-image-base-1.0,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/core-image-base-1.0 PSEUDO_DISABLED=0 /home/parallels/yocto/build/tmp/sysroots-components/aarch64/pseudo-native/usr/bin/pseudo -S
}

do_image_ext3() {
	oe_mkext234fs ext3 -i 4096
	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete
}

do_image_tar() {
	tar --sort=name --format=posix --numeric-owner -cf /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.tar -C /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs . || [ $? -eq 1 ]
	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete
	pbzip2 -f -k core-image-base-raspberrypi4-64-20240108202948.rootfs.tar
	rm core-image-base-raspberrypi4-64-20240108202948.rootfs.tar
}

do_image_wic() {
		out="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948"
	build_wic="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/build-wic"
	tmp_wic="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/tmp-wic"
	wks="/home/parallels/yocto/meta-raspberrypi/wic/sdimage-raspberrypi.wks"
	if [ -e "$tmp_wic" ]; then
		# Ensure we don't have any junk leftover from a previously interrupted
		# do_image_wic execution
		rm -rf "$tmp_wic"
	fi
	if [ -z "$wks" ]; then
		bbfatal "No kickstart files from WKS_FILES were found: sdimage-raspberrypi.wks core-image-base.wks. Please set WKS_FILE or WKS_FILES appropriately."
	fi
	BUILDDIR="/home/parallels/yocto/build" PSEUDO_UNLOAD=1 wic create "$wks" --vars "/home/parallels/yocto/build/tmp/sysroots/raspberrypi4-64/imgdata/" -e "core-image-base" -o "$build_wic/" -w "$tmp_wic" 
	mv "$build_wic/$(basename "${wks%.wks}")"*.direct "$out.rootfs.wic"

	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete
	pbzip2 -f -k core-image-base-raspberrypi4-64-20240108202948.rootfs.wic
	bmaptool create core-image-base-raspberrypi4-64-20240108202948.rootfs.wic -o core-image-base-raspberrypi4-64-20240108202948.rootfs.wic.bmap
	rm core-image-base-raspberrypi4-64-20240108202948.rootfs.wic
}

do_install() {
    base_do_install
}

empty_var_volatile() {
	if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/fstab ]; then
		match=`awk '$1 !~ "#" && $2 ~ /\/var\/volatile/{print $2}' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/fstab 2> /dev/null`
		if [ -n "$match" ]; then
			find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/var/volatile -mindepth 1 -delete
		fi
	fi
}

hardlinkdir() {
	from=$1
	to=$2
	(cd $from; find . -print0 | cpio --null -pdlu $to)
}

install_tools() {
	install -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/sysroots/aarch64-pokysdk-linux/usr/bin
	scripts="devtool recipetool oe-find-native-sysroot runqemu* wic"
	for script in $scripts; do
		for scriptfn in `find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/${scriptrelpath} -maxdepth 1 -executable -name "$script"`; do
			targetscriptfn="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/sysroots/aarch64-pokysdk-linux/usr/bin/$(basename $scriptfn)"
			test -e ${targetscriptfn} || lnr ${scriptfn} ${targetscriptfn}
		done
	done
	# We can't use the same method as above because files in the sysroot won't exist at this point
	# (they get populated from sstate on installation)
	unfsd_path="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/sysroots/aarch64-pokysdk-linux/usr/bin/unfsd"
	if [ "1" = "1" -a ! -e $unfsd_path ] ; then
		binrelpath=work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot-native/usr/bin
		lnr /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/tmp/$binrelpath/unfsd $unfsd_path
	fi
	touch /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/.devtoolbase

	# find latest buildtools-tarball and install it
	if [ -n "${SDK_BUILDTOOLS_INSTALLER}" ]; then
		install /home/parallels/yocto/build/tmp/deploy/sdk/${SDK_BUILDTOOLS_INSTALLER} /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath
	fi

	install -m 0644 /home/parallels/yocto/poky/meta/files/ext-sdk-prepare.py /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath
}

ldconfig_postinst_fragment() {
if [ x"$D" = "x" ]; then
	if [ -x /sbin/ldconfig ]; then /sbin/ldconfig ; fi
fi
}

make_zimage_symlink_relative() {
	if [ -L /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/boot/zImage ]; then
		(cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/boot/ && for i in `ls zImage-* | sort`; do ln -sf $i zImage; done)
	fi
}

mklibs_optimize_image() {
	for img in ${MKLIBS_OPTIMIZED_IMAGES}
	do
		if [ "${img}" = "core-image-base" ] || [ "${img}" = "all" ]
		then
			mklibs_optimize_image_doit
			break
		fi
	done
}

mklibs_optimize_image_doit() {
	rm -rf /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs
	mkdir -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/dest
	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs
	du -bs > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/du.before.mklibs.txt

	# Build a list of dynamically linked executable ELF files.
	# Omit libc/libpthread as a special case because it has an interpreter
	# but is primarily what we intend to strip down.
	for i in `find . -type f -executable ! -name 'libc-*' ! -name 'libpthread-*'`; do
		file $i | grep -q ELF || continue
		aarch64-poky-linux-readelf -l $i | grep -q INTERP || continue
		echo $i
	done > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/executables.list

	dynamic_loader=/lib/ld-linux-aarch64.so.1

	mklibs -v \
		--ldlib ${dynamic_loader} \
		--libdir lib \
		--sysroot /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot \
		--gcc-options "--sysroot=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot" \
		--root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs \
		--target `echo aarch64-poky-linux- | sed 's/-$//' ` \
		-d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/dest \
		`cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/executables.list`

	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/dest
	for i in *
	do
		cp $i `find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -name $i`
	done

	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs
	du -bs > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/du.after.mklibs.txt

	echo rootfs size before mklibs optimization: `cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/du.before.mklibs.txt`
	echo rootfs size after mklibs optimization: `cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/mklibs/du.after.mklibs.txt`
}

multiubi_mkfs() {
	local mkubifs_args="$1"
	local ubinize_args="$2"

        # Added prompt error message for ubi and ubifs image creation.
        if [ -z "$mkubifs_args" ] || [ -z "$ubinize_args" ]; then
            bbfatal "MKUBIFS_ARGS and UBINIZE_ARGS have to be set, see http://www.linux-mtd.infradead.org/faq/ubifs.html for details"
        fi

	if [ -z "$3" ]; then
		local vname=""
	else
		local vname="_$3"
	fi

	echo \[ubifs\] > ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo mode=ubi >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo image=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubifs >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo vol_id=0 >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo vol_type=dynamic >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo vol_name=raspberrypi4-64-rootfs >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	echo vol_flags=autoresize >> ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg
	if [ -n "$vname" ]; then
		mkfs.ubifs -r /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -o /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubifs ${mkubifs_args}
	fi
	ubinize -o /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubi ${ubinize_args} ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg

	# Cleanup cfg file
	mv ubinize${vname}-core-image-base-raspberrypi4-64-20240108202948.cfg /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/

	# Create own symlinks for 'named' volumes
	if [ -n "$vname" ]; then
		cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete
		if [ -e core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubifs ]; then
			ln -sf core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubifs \
			core-image-base-raspberrypi4-64${vname}.ubifs
		fi
		if [ -e core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubi ]; then
			ln -sf core-image-base-raspberrypi4-64-20240108202948${vname}.rootfs.ubi \
			core-image-base-raspberrypi4-64${vname}.ubi
		fi
		cd -
	fi
}

oe_libinstall() {
	# Purpose: Install a library, in all its forms
	# Example
	#
	# oe_libinstall libltdl /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot/usr/lib/
	# oe_libinstall -C src/libblah libblah /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image//usr/lib/
	dir=""
	libtool=""
	silent=""
	require_static=""
	require_shared=""
	staging_install=""
	while [ "$#" -gt 0 ]; do
		case "$1" in
		-C)
			shift
			dir="$1"
			;;
		-s)
			silent=1
			;;
		-a)
			require_static=1
			;;
		-so)
			require_shared=1
			;;
		-*)
			bbfatal "oe_libinstall: unknown option: $1"
			;;
		*)
			break;
			;;
		esac
		shift
	done

	libname="$1"
	shift
	destpath="$1"
	if [ -z "$destpath" ]; then
		bbfatal "oe_libinstall: no destination path specified"
	fi
	if echo "$destpath/" | egrep '^/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot/usr/lib/' >/dev/null
	then
		staging_install=1
	fi

	__runcmd () {
		if [ -z "$silent" ]; then
			echo >&2 "oe_libinstall: $*"
		fi
		$*
	}

	if [ -z "$dir" ]; then
		dir=`pwd`
	fi

	dotlai=$libname.lai

	# Sanity check that the libname.lai is unique
	number_of_files=`(cd $dir; find . -name "$dotlai") | wc -l`
	if [ $number_of_files -gt 1 ]; then
		bbfatal "oe_libinstall: $dotlai is not unique in $dir"
	fi


	dir=$dir`(cd $dir;find . -name "$dotlai") | sed "s/^\.//;s/\/$dotlai\$//;q"`
	olddir=`pwd`
	__runcmd cd $dir

	lafile=$libname.la

	# If such file doesn't exist, try to cut version suffix
	if [ ! -f "$lafile" ]; then
		libname1=`echo "$libname" | sed 's/-[0-9.]*$//'`
		lafile1=$libname.la
		if [ -f "$lafile1" ]; then
			libname=$libname1
			lafile=$lafile1
		fi
	fi

	if [ -f "$lafile" ]; then
		# libtool archive
		eval `cat $lafile|grep "^library_names="`
		libtool=1
	else
		library_names="$libname.so* $libname.dll.a $libname.*.dylib"
	fi

	__runcmd install -d $destpath/
	dota=$libname.a
	if [ -f "$dota" -o -n "$require_static" ]; then
		rm -f $destpath/$dota
		__runcmd install -m 0644 $dota $destpath/
	fi
	if [ -f "$dotlai" -a -n "$libtool" ]; then
		rm -f $destpath/$libname.la
		__runcmd install -m 0644 $dotlai $destpath/$libname.la
	fi

	for name in $library_names; do
		files=`eval echo $name`
		for f in $files; do
			if [ ! -e "$f" ]; then
				if [ -n "$libtool" ]; then
					bbfatal "oe_libinstall: $dir/$f not found."
				fi
			elif [ -L "$f" ]; then
				__runcmd cp -P "$f" $destpath/
			elif [ ! -L "$f" ]; then
				libfile="$f"
				rm -f $destpath/$libfile
				__runcmd install -m 0755 $libfile $destpath/
			fi
		done
	done

	if [ -z "$libfile" ]; then
		if  [ -n "$require_shared" ]; then
			bbfatal "oe_libinstall: unable to locate shared library"
		fi
	elif [ -z "$libtool" ]; then
		# special case hack for non-libtool .so.#.#.# links
		baselibfile=`basename "$libfile"`
		if (echo $baselibfile | grep -qE '^lib.*\.so\.[0-9.]*$'); then
			sonamelink=`aarch64-poky-linux-readelf -d $libfile |grep 'Library soname:' |sed -e 's/.*\[\(.*\)\].*/\1/'`
			solink=`echo $baselibfile | sed -e 's/\.so\..*/.so/'`
			if [ -n "$sonamelink" -a x"$baselibfile" != x"$sonamelink" ]; then
				__runcmd ln -sf $baselibfile $destpath/$sonamelink
			fi
			__runcmd ln -sf $baselibfile $destpath/$solink
		fi
	fi

	__runcmd cd "$olddir"
}

oe_machinstall() {
	# Purpose: Install machine dependent files, if available
	#          If not available, check if there is a default
	#          If no default, just touch the destination
	# Example:
	#                $1  $2   $3         $4
	# oe_machinstall -m 0644 fstab /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image/etc/fstab
	#
	# TODO: Check argument number?
	#
	filename=`basename $3`
	dirname=`dirname $3`

	for o in `echo linux:aarch64:pn-core-image-base:rpi:aarch64:raspberrypi4:raspberrypi4-64:poky:class-target:libc-glibc:forcevariable | tr ':' ' '`; do
		if [ -e $dirname/$o/$filename ]; then
			bbnote $dirname/$o/$filename present, installing to $4
			install $1 $2 $dirname/$o/$filename $4
			return
		fi
	done
#	bbnote overrides specific file NOT present, trying default=$3...
	if [ -e $3 ]; then
		bbnote $3 present, installing to $4
		install $1 $2 $3 $4
	else
		bbnote $3 NOT present, touching empty $4
		touch $4
	fi
}

oe_mkext234fs() {
	fstype=$1
	extra_imagecmd=""

	if [ $# -gt 1 ]; then
		shift
		extra_imagecmd=$@
	fi

	# If generating an empty image the size of the sparse block should be large
	# enough to allocate an ext4 filesystem using 4096 bytes per inode, this is
	# about 60K, so dd needs a minimum count of 60, with bs=1024 (bytes per IO)
	eval local COUNT=\"0\"
	eval local MIN_COUNT=\"60\"
	if [ $ROOTFS_SIZE -lt $MIN_COUNT ]; then
		eval COUNT=\"$MIN_COUNT\"
	fi
	# Create a sparse image block
	bbdebug 1 Executing "dd if=/dev/zero of=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype seek=$ROOTFS_SIZE count=$COUNT bs=1024"
	dd if=/dev/zero of=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype seek=$ROOTFS_SIZE count=$COUNT bs=1024
	bbdebug 1 "Actual Rootfs size:  `du -s /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs`"
	bbdebug 1 "Actual Partion size: `stat -c '%s' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype`"
	bbdebug 1 Executing "mkfs.$fstype -F $extra_imagecmd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs"
	mkfs.$fstype -F $extra_imagecmd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs
	# Error codes 0-3 indicate successfull operation of fsck (no errors or errors corrected)
	fsck.$fstype -pvfD /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/deploy-core-image-base-image-complete/core-image-base-raspberrypi4-64-20240108202948.rootfs.$fstype || [ $? -le 3 ]
}

oe_runmake() {
	oe_runmake_call "$@" || die "oe_runmake failed"
}

oe_runmake_call() {
	bbnote make  "$@"
	make  "$@"
}

oe_soinstall() {
	# Purpose: Install shared library file and
	#          create the necessary links
	# Example: oe_soinstall libfoo.so.1.2.3 /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image/usr/lib
	libname=`basename $1`
	case "$libname" in
	    *.so)
	        bbfatal "oe_soinstall: Shared library must haved versioned filename (e.g. libfoo.so.1.2.3)"
	        ;;
	esac
	install -m 755 $1 $2/$libname
	sonamelink=`aarch64-poky-linux-readelf -d $1 |grep 'Library soname:' |sed -e 's/.*\[\(.*\)\].*/\1/'`
	if [ -z $sonamelink ]; then
		bbfatal "oe_soinstall: $libname is missing ELF tag 'SONAME'."
	fi
	solink=`echo $libname | sed -e 's/\.so\..*/.so/'`
	ln -sf $libname $2/$sonamelink
	ln -sf $libname $2/$solink
}

packagedata_translate_pr_autoinc() {
    find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/pkgdata-pdata-input -type f | xargs --no-run-if-empty \
        sed -e 's,@PRSERV_PV_AUTOINC@,AUTOINC,g' \
            -e 's,@EXTENDPRAUTO@,,g' -i
}

populate_sdk_log_check() {
	for target in $*
	do
		lf_path="`dirname ${BB_LOGFILE}`/log.do_$target.3508382"

		echo "log_check: Using $lf_path as logfile"

		if [ -e "$lf_path" ]; then
			rpm_log_check $target $lf_path
		else
			echo "Cannot find logfile [$lf_path]"
		fi
		echo "Logfile is clean"
	done
}

postinst_enable_logging() {
	mkdir -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default
	echo "POSTINST_LOGGING=1" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/postinst
	echo "LOGFILE=/var/log/postinstall.log" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/postinst
}

prelink_image() {
#	export PSEUDO_DEBUG=4
#	/bin/env | /bin/grep PSEUDO
#	echo "LD_LIBRARY_PATH=$LD_LIBRARY_PATH"
#	echo "LD_PRELOAD=$LD_PRELOAD"

	pre_prelink_size=`du -ks /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs | awk '{size = $1 ; print size }'`
	echo "Size before prelinking $pre_prelink_size."

	# The filesystem may not contain sysconfdir so establish what is present
	# to enable cleanup after temporary creation of sysconfdir if needed
	presentdir="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc"
	while [ "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs" != "${presentdir}" ] ; do
		[ ! -d "${presentdir}" ] || break
		presentdir=`dirname "${presentdir}"`
	done

	mkdir -p "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc"

	# We need a prelink conf on the filesystem, add one if it's missing
	if [ ! -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/prelink.conf ]; then
		cp /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot-native/etc/prelink.conf \
			/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/prelink.conf
		dummy_prelink_conf=true;
	else
		dummy_prelink_conf=false;
	fi

	# We need a ld.so.conf with pathnames in,prelink conf on the filesystem, add one if it's missing
	ldsoconf=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ld.so.conf
	if [ -e $ldsoconf ]; then
		cp $ldsoconf $ldsoconf.prelink
	fi
	cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot/etc/ld.so.conf >> $ldsoconf

	dynamic_loader=/lib/ld-linux-aarch64.so.1

	# prelink!
	if [ "1" = "1" ]; then
		bbnote " prelink: BUILD_REPRODUCIBLE_BINARIES..."
		if [ "$REPRODUCIBLE_TIMESTAMP_ROOTFS" = "" ]; then
			export PRELINK_TIMESTAMP=`git log -1 --pretty=%ct `
		else
			export PRELINK_TIMESTAMP=$REPRODUCIBLE_TIMESTAMP_ROOTFS
		fi
		/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot-native/usr/sbin/prelink --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -am -N -c /etc/prelink.conf --dynamic-linker $dynamic_loader
	else
		/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/recipe-sysroot-native/usr/sbin/prelink --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -amR -N -c /etc/prelink.conf --dynamic-linker $dynamic_loader
	fi

	# Remove the prelink.conf if we had to add it.
	if [ "$dummy_prelink_conf" = "true" ]; then
		rm -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/prelink.conf
	fi

	if [ -e $ldsoconf.prelink ]; then
		mv $ldsoconf.prelink $ldsoconf
	else
		rm $ldsoconf
	fi

	# Remove any directories temporarily created for sysconfdir
	cleanupdir="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc"
	while [ "${presentdir}" != "${cleanupdir}" ] ; do
		rmdir "${cleanupdir}"
		cleanupdir=`dirname ${cleanupdir}`
	done

	pre_prelink_size=`du -ks /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs | awk '{size = $1 ; print size }'`
	echo "Size after prelinking $pre_prelink_size."
}

read_only_rootfs_hook() {
	# Tweak the mount option and fs_passno for rootfs in fstab
	if [ -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/fstab ]; then
		sed -i -e '/^[#[:space:]]*\/dev\/root/{s/defaults/ro/;s/\([[:space:]]*[[:digit:]]\)\([[:space:]]*\)[[:digit:]]$/\1\20/}' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/fstab
	fi

	# Tweak the "mount -o remount,rw /" command in busybox-inittab inittab
	if [ -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/inittab ]; then
		sed -i 's|/bin/mount -o remount,rw /|/bin/mount -o remount,ro /|' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/inittab
	fi

	# If we're using openssh and the /etc/ssh directory has no pre-generated keys,
	# we should configure openssh to use the configuration file /etc/ssh/sshd_config_readonly
	# and the keys under /var/run/ssh.
	if [ -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh ]; then
		if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh/ssh_host_rsa_key ]; then
			echo "SYSCONFDIR=\${SYSCONFDIR:-/etc/ssh}" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/ssh
			echo "SSHD_OPTS=" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/ssh
		else
			echo "SYSCONFDIR=\${SYSCONFDIR:-/var/run/ssh}" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/ssh
			echo "SSHD_OPTS='-f /etc/ssh/sshd_config_readonly'" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/ssh
		fi
	fi

	# Also tweak the key location for dropbear in the same way.
	if [ -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/dropbear ]; then
		if [ ! -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/dropbear/dropbear_rsa_host_key ]; then
			echo "DROPBEAR_RSAKEY_DIR=/var/lib/dropbear" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear
		fi
	fi

	if true; then
		# Change the value of ROOTFS_READ_ONLY in /etc/default/rcS to yes
		if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/rcS ]; then
			sed -i 's/ROOTFS_READ_ONLY=no/ROOTFS_READ_ONLY=yes/' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/rcS
		fi
		# Run populate-volatile.sh at rootfs time to set up basic files
		# and directories to support read-only rootfs.
		if [ -x /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/init.d/populate-volatile.sh ]; then
			/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/init.d/populate-volatile.sh
		fi
	fi

	if false; then
	# Create machine-id
	# 20:12 < mezcalero> koen: you have three options: a) run systemd-machine-id-setup at install time, b) have / read-only and an empty file there (for stateless) and c) boot with / writable
		touch /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/machine-id
	fi
}

remove_init_link() {
	if [ -h /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init ]; then
		LINKFILE=/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs`readlink /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init`
		rm /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init
		cp $LINKFILE /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/sbin/init
	fi
}

remove_libtool_la() {
	if [ "1" != "0" ]; then
		find "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image" -ignore_readdir_race -name "*.la" -delete
	fi
}

reproducible_final_image_task() {
    if [ "1" = "1" ]; then
        if [ "$REPRODUCIBLE_TIMESTAMP_ROOTFS" = "" ]; then
            REPRODUCIBLE_TIMESTAMP_ROOTFS=`git -C "/home/parallels/yocto/poky" log -1 --pretty=%ct 2>/dev/null` || true
            if [ "$REPRODUCIBLE_TIMESTAMP_ROOTFS" = "" ]; then
                REPRODUCIBLE_TIMESTAMP_ROOTFS=`stat -c%Y /home/parallels/yocto/poky/meta/conf/bitbake.conf`
            fi
        fi
        # Set mtime of all files to a reproducible value
        bbnote "reproducible_final_image_task: mtime set to $REPRODUCIBLE_TIMESTAMP_ROOTFS"
        find  /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -print0 | xargs -0 touch -h  --date=@$REPRODUCIBLE_TIMESTAMP_ROOTFS
    fi
}

rootfs_check_host_user_contaminated() {
	contaminated="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/core-image-base-1.0/host-user-contaminated.txt"
	HOST_USER_UID="$(PSEUDO_UNLOAD=1 id -u)"
	HOST_USER_GID="$(PSEUDO_UNLOAD=1 id -g)"

	find "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs" -path "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/home" -prune -o \
	    -user "$HOST_USER_UID" -print -o -group "$HOST_USER_GID" -print >"$contaminated"

	sed -e "s,/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs,," $contaminated | while read line; do
		bbwarn "Path in the rootfs is owned by the same user or group as the user running bitbake:" $line `ls -lan /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/$line`
	done

	if [ -s "$contaminated" ]; then
		bbwarn "/etc/passwd:" `cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/passwd`
		bbwarn "/etc/group:" `cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/group`
	fi
}

rootfs_no_x_startup() {
	if [ -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/init.d/xserver-nodm ]; then
		chmod a-x /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/init.d/xserver-nodm
	fi
}

rootfs_reproducible() {
	if [ "1520598896" != "" ]; then
		# Convert UTC into %4Y%2m%2d%2H%2M%2S
		sformatted=`date -u -d @1520598896 +%4Y%2m%2d%2H%2M%2S`
		echo $sformatted > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/version
		bbnote "rootfs_reproducible: set /etc/version to $sformatted"

		if [ -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/gconf ]; then
			find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/gconf -name '%gconf.xml' -print0 | xargs -0r \
			sed -i -e 's@\bmtime="[0-9][0-9]*"@mtime="'1520598896'"@g'
		fi
	fi
}

rootfs_sysroot_relativelinks() {
	sysroot-relativelinks.py /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/sysroots/aarch64-poky-linux
}

rootfs_trim_schemas() {
	for schema in /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/gconf/schemas/*.schemas
	do
		# Need this in case no files exist
		if [ -e $schema ]; then
			oe-trim-schemas $schema > $schema.new
			mv $schema.new $schema
		fi
	done
}

rootfs_update_timestamp() {
	if [ "1520598896" != "" ]; then
		# Convert UTC into %4Y%2m%2d%2H%2M%2S
		sformatted=`date -u -d @1520598896 +%4Y%2m%2d%2H%2M%2S`
	else
		sformatted=`date -u +%4Y%2m%2d%2H%2M%2S`
	fi
	echo $sformatted > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/timestamp
	bbnote "rootfs_update_timestamp: set /etc/timestamp to $sformatted"
}

rpi_generate_sysctl_config() {
    # systemd sysctl config
    test -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/sysctl.d && \
        echo "vm.min_free_kbytes = 8192" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/sysctl.d/rpi-vm.conf

    # sysv sysctl config
    IMAGE_SYSCTL_CONF="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/sysctl.conf"
    test -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/sysctl.conf && \
        sed -e "/vm.min_free_kbytes/d" -i ${IMAGE_SYSCTL_CONF}
    echo "" >> ${IMAGE_SYSCTL_CONF} && echo "vm.min_free_kbytes = 8192" >> ${IMAGE_SYSCTL_CONF}
}

sdk_ext_postinst() {
	printf "\nExtracting buildtools...\n"
	cd $target_sdk_dir
	env_setup_script="$target_sdk_dir/environment-setup-aarch64-poky-linux"
        if [ -n "${SDK_BUILDTOOLS_INSTALLER}" ]; then
		printf "buildtools\ny" | ./${SDK_BUILDTOOLS_INSTALLER} > buildtools.log || { printf 'ERROR: buildtools installation failed:\n' ; cat buildtools.log ; echo "printf 'ERROR: this SDK was not fully installed and needs reinstalling\n'" >> $env_setup_script ; exit 1 ; }

		# Delete the buildtools tar file since it won't be used again
		rm -f ./${SDK_BUILDTOOLS_INSTALLER}
		# We don't need the log either since it succeeded
		rm -f buildtools.log

		# Make sure when the user sets up the environment, they also get
		# the buildtools-tarball tools in their path.
		echo ". $target_sdk_dir/buildtools/environment-setup*" >> $env_setup_script
	fi

	# Allow bitbake environment setup to be ran as part of this sdk.
	echo "export OE_SKIP_SDK_CHECK=1" >> $env_setup_script
	# Work around runqemu not knowing how to get this information within the eSDK
	echo "export DEPLOY_DIR_IMAGE=$target_sdk_dir/tmp/deploy/images/raspberrypi4-64" >> $env_setup_script

	# A bit of another hack, but we need this in the path only for devtool
	# so put it at the end of $PATH.
	echo "export PATH=\"$target_sdk_dir/sysroots/aarch64-pokysdk-linux/usr/bin:\$PATH\"" >> $env_setup_script

	echo "printf 'SDK environment now set up; additionally you may now run devtool to perform development tasks.\nRun devtool --help for further details.\n'" >> $env_setup_script

	# Warn if trying to use external bitbake and the ext SDK together
	echo "(which bitbake > /dev/null 2>&1 && echo 'WARNING: attempting to use the extensible SDK in an environment set up to run bitbake - this may lead to unexpected results. Please source this script in a new shell session instead.') || true" >> $env_setup_script

	if [ "$prepare_buildsystem" != "no" -a -n "${SDK_BUILDTOOLS_INSTALLER}" ]; then
		printf "Preparing build system...\n"
		# dash which is /bin/sh on Ubuntu will not preserve the
		# current working directory when first ran, nor will it set $1 when
		# sourcing a script. That is why this has to look so ugly.
		LOGFILE="$target_sdk_dir/preparing_build_system.log"
		sh -c ". buildtools/environment-setup* > $LOGFILE && cd $target_sdk_dir/`dirname ${oe_init_build_env_path}` && set $target_sdk_dir && . $target_sdk_dir/${oe_init_build_env_path} $target_sdk_dir >> $LOGFILE && python3 $target_sdk_dir/ext-sdk-prepare.py $LOGFILE ''" || { echo "printf 'ERROR: this SDK was not fully installed and needs reinstalling\n'" >> $env_setup_script ; exit 1 ; }
	fi
	if [ -e $target_sdk_dir/ext-sdk-prepare.py ]; then
		rm $target_sdk_dir/ext-sdk-prepare.py
	fi
	echo done
}

sdk_ext_preinst() {
	# Since bitbake won't run as root it doesn't make sense to try and install
	# the extensible sdk as root.
	if [ "`id -u`" = "0" ]; then
		echo "ERROR: The extensible sdk cannot be installed as root."
		exit 1
	fi
	if ! command -v locale > /dev/null; then
		echo "ERROR: The installer requires the locale command, please install it first"
		exit 1
	fi
        # Check setting of LC_ALL set above
	canonicalised_locale=`echo $LC_ALL | sed 's/UTF-8/utf8/'`
	if ! locale -a | grep -q $canonicalised_locale ; then
		echo "ERROR: the installer requires the $LC_ALL locale to be installed (but not selected), please install it first"
		exit 1
	fi
	# The relocation script used by buildtools installer requires python
	if ! command -v python3 > /dev/null; then
		echo "ERROR: The installer requires python3, please install it first"
		exit 1
	fi
	missing_utils=""
	for util in ${SDK_REQUIRED_UTILITIES}; do
		if ! command -v $util > /dev/null; then
			missing_utils="$missing_utils $util"
		fi
	done
	if [ -n "$missing_utils" ] ; then
		echo "ERROR: the SDK requires the following missing utilities, please install them: $missing_utils"
		exit 1
	fi
	SDK_EXTENSIBLE="1"
	if [ "$publish" = "1" ] && [ "full" = "minimal" ] ; then
		EXTRA_TAR_OPTIONS="$EXTRA_TAR_OPTIONS --exclude=sstate-cache"
	fi
}

set_systemd_default_target() {
	if [ -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/systemd/system -a -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/lib/systemd/system/multi-user.target ]; then
		ln -sf /lib/systemd/system/multi-user.target /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/systemd/system/default.target
	fi
}

ssh_allow_empty_password() {
	for config in sshd_config sshd_config_readonly; do
		if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh/$config ]; then
			sed -i 's/^[#[:space:]]*PermitEmptyPasswords.*/PermitEmptyPasswords yes/' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh/$config
		fi
	done

	if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/usr/sbin/dropbear ] ; then
		if grep -q DROPBEAR_EXTRA_ARGS /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear 2>/dev/null ; then
			if ! grep -q "DROPBEAR_EXTRA_ARGS=.*-B" /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear ; then
				sed -i 's/^DROPBEAR_EXTRA_ARGS="*\([^"]*\)"*/DROPBEAR_EXTRA_ARGS="\1 -B"/' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear
			fi
		else
			printf '\nDROPBEAR_EXTRA_ARGS="-B"\n' >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear
		fi
	fi

	if [ -d /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/pam.d ] ; then
		for f in `find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/pam.d/* -type f -exec test -e {} \; -print`
		do
			sed -i 's/nullok_secure/nullok/' $f
		done
	fi
}

ssh_allow_root_login() {
	for config in sshd_config sshd_config_readonly; do
		if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh/$config ]; then
			sed -i 's/^[#[:space:]]*PermitRootLogin.*/PermitRootLogin yes/' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/ssh/$config
		fi
	done

	if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/usr/sbin/dropbear ] ; then
		if grep -q DROPBEAR_EXTRA_ARGS /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear 2>/dev/null ; then
			sed -i '/^DROPBEAR_EXTRA_ARGS=/ s/-w//' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/default/dropbear
		fi
	fi
}

sstate_create_package() {
	# Exit early if it already exists
	if [ -e /home/parallels/yocto/build/sstate-cache/ ]; then
		touch /home/parallels/yocto/build/sstate-cache/ 2>/dev/null || true
		return
	fi

	mkdir -p `dirname /home/parallels/yocto/build/sstate-cache/`
	TFILE=`mktemp /home/parallels/yocto/build/sstate-cache/.XXXXXXXX`

	# Use pigz if available
	OPT="-czS"
	if [ -x "$(command -v pigz)" ]; then
		OPT="-I pigz -cS"
	fi

	# Need to handle empty directories
	if [ "$(ls -A)" ]; then
		set +e
		tar $OPT -f $TFILE *
		ret=$?
		if [ $ret -ne 0 ] && [ $ret -ne 1 ]; then
			exit 1
		fi
		set -e
	else
		tar $OPT --file=$TFILE --files-from=/dev/null
	fi
	chmod 0664 $TFILE
	# Skip if it was already created by some other process
	if [ -h /home/parallels/yocto/build/sstate-cache/ ] && [ ! -e /home/parallels/yocto/build/sstate-cache/ ]; then
		# There is a symbolic link, but it links to nothing.
		# Forcefully replace it with the new file.
		ln -f $TFILE /home/parallels/yocto/build/sstate-cache/ || true
	elif [ ! -e /home/parallels/yocto/build/sstate-cache/ ]; then
		# Move into place using ln to attempt an atomic op.
		# Abort if it already exists
		ln $TFILE /home/parallels/yocto/build/sstate-cache/ || true
	else
		touch /home/parallels/yocto/build/sstate-cache/ 2>/dev/null || true
	fi
	rm $TFILE
}

sstate_unpack_package() {
	tar -xvzf /home/parallels/yocto/build/sstate-cache/
	# update .siginfo atime on local/NFS mirror if it is a symbolic link
	[ ! -h /home/parallels/yocto/build/sstate-cache/.siginfo ] || [ ! -e /home/parallels/yocto/build/sstate-cache/.siginfo ] || touch -a /home/parallels/yocto/build/sstate-cache/.siginfo 2>/dev/null || true
	# update each symbolic link instead of any referenced file
	touch --no-dereference /home/parallels/yocto/build/sstate-cache/ 2>/dev/null || true
	[ ! -e /home/parallels/yocto/build/sstate-cache/.sig ] || touch --no-dereference /home/parallels/yocto/build/sstate-cache/.sig 2>/dev/null || true
	[ ! -e /home/parallels/yocto/build/sstate-cache/.siginfo ] || touch --no-dereference /home/parallels/yocto/build/sstate-cache/.siginfo 2>/dev/null || true
}

sysroot_stage_all() {
	sysroot_stage_dirs /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sysroot-destdir
}

sysroot_stage_dir() {
	src="$1"
	dest="$2"
	# if the src doesn't exist don't do anything
	if [ ! -d "$src" ]; then
		 return
	fi

	mkdir -p "$dest"
	(
		cd $src
		find . -print0 | cpio --null -pdlu $dest
	)
}

sysroot_stage_dirs() {
	from="$1"
	to="$2"

	for dir in      /usr/include     /usr/lib     /lib     /lib     /usr/share ; do
		sysroot_stage_dir "$from$dir" "$to$dir"
	done

	# Remove directories we do not care about
	for dir in      /usr/share/man     /usr/share/doc     /usr/share/info     /usr/share/X11/locale     /usr/share/applications     /usr/share/bash-completion     /usr/share/fonts     /usr/share/gtk-doc/html     /usr/share/installed-tests     /usr/share/locale     /usr/share/pixmaps     /usr/share/terminfo     /usr/lib/core-image-base/ptest ; do
		rm -rf "$to$dir"
	done
}

systemd_create_users() {
	for conffile in /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/usr/lib/sysusers.d/systemd.conf /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/usr/lib/sysusers.d/systemd-remote.conf; do
		[ -e $conffile ] || continue
		grep -v "^#" $conffile | sed -e '/^$/d' | while read type name id comment; do
		if [ "$type" = "u" ]; then
			useradd_params="--shell /sbin/nologin"
			[ "$id" != "-" ] && useradd_params="$useradd_params --uid $id"
			[ "$comment" != "-" ] && useradd_params="$useradd_params --comment $comment"
			useradd_params="$useradd_params --system $name"
			eval useradd --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs $useradd_params || true
		elif [ "$type" = "g" ]; then
			groupadd_params=""
			[ "$id" != "-" ] && groupadd_params="$groupadd_params --gid $id"
			groupadd_params="$groupadd_params --system $name"
			eval groupadd --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs $groupadd_params || true
		elif [ "$type" = "m" ]; then
			group=$id
			if [ ! `grep -q "^${group}:" /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/group` ]; then
				eval groupadd --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs --system $group
			fi
			if [ ! `grep -q "^${name}:" /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/passwd` ]; then
				eval useradd --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs --shell /sbin/nologin --system $name
			fi
			eval usermod --root /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs -a -G $group $name
		fi
		done
	done
}

systemd_preset_all() {
    if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/lib/systemd/systemd ]; then
	systemctl --root="/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs" --preset-mode=enable-only preset-all
    fi
}

zap_empty_root_password() {
	if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/shadow ]; then
		sed -i 's%^root::%root:*:%' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/shadow
        fi
	if [ -e /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/passwd ]; then
		sed -i 's%^root::%root:*:%' /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs/etc/passwd
	fi
}



