}

archive_sdk() {
	# Package it up
	mkdir -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk
	cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath; tar --owner=root --group=root -cf - . | xz --memlimit=50% --threads=6 -9 > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.tar.xz
}

base_do_compile() {
	if [ -e Makefile -o -e makefile -o -e GNUmakefile ]; then
		oe_runmake || die "make failed"
	else
		bbnote "nothing to compile"
	fi
}

base_do_configure() {
	if [ -n "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate" -a -e "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate" ]; then
		if [ "`cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate`" != "${BB_TASKHASH}" ]; then
			cd /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/core-image-base-1.0
			if [ "0" != "1" -a \( -e Makefile -o -e makefile -o -e GNUmakefile \) ]; then
				oe_runmake clean
			fi
			# -ignore_readdir_race does not work correctly with -delete;
			# use xargs to avoid spurious build failures
			find /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/core-image-base-1.0 -ignore_readdir_race -name \*.la -type f -print0 | xargs -0 rm -f
		fi
	fi
	if [ -n "/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate" ]; then
		mkdir -p `dirname /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate`
		echo ${BB_TASKHASH} > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/configure.sstate
	fi
}

base_do_install() {
	:
}

base_do_package() {
	:
}

bbdebug() {
	USAGE='Usage: bbdebug [123] "message"'
	if [ $# -lt 2 ]; then
		bbfatal "$USAGE"
	fi

	# Strip off the debug level and ensure it is an integer
	DBGLVL=$1; shift
	NONDIGITS=$(echo "$DBGLVL" | tr -d "[:digit:]")
	if [ "$NONDIGITS" ]; then
		bbfatal "$USAGE"
	fi

	# All debug output is printed to the logs
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbdebug $DBGLVL $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "DEBUG: $*"
	fi
}

bberror() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bberror $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "ERROR: $*"
	fi
}

bbfatal() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbfatal $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "ERROR: $*"
	fi
	exit 1
}

bbfatal_log() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbfatal_log $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "ERROR: $*"
	fi
	exit 1
}

bbnote() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbnote $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "NOTE: $*"
	fi
}

bbplain() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbplain $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "$*"
	fi
}

bbwarn() {
	if [ -p /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382 ] ; then
		printf "%b\0" "bbwarn $*" > /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/fifo.3508382
	else
		echo "WARNING: $*"
	fi
}

check_git_config() {
	if ! git config user.email > /dev/null ; then
		git config --local user.email "oe.patch@oe"
	fi
	if ! git config user.name > /dev/null ; then
		git config --local user.name "OpenEmbedded"
	fi
}

create_cmdline_wrapper() {
	# Create a wrapper script where commandline options are needed
	#
	# These are useful to work around relocation issues, by passing extra options
	# to a program
	#
	# Usage: create_cmdline_wrapper FILENAME <extra-options>

	cmd=$1
	shift

	echo "Generating wrapper script for $cmd"

	mv $cmd $cmd.real
	cmdname=`basename $cmd`
	dirname=`dirname $cmd`
	cmdoptions=$@
	if [ "" != "" ]; then
		relpath=`python3 -c "import os; print(os.path.relpath('/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image', '$dirname'))"`
		cmdoptions=`echo $@ | sed -e "s::\\$realdir/$relpath:g"`
	fi
	cat <<END >$cmd
#!/bin/bash
realpath=\`readlink -fn \$0\`
realdir=\`dirname \$realpath\`
exec -a \$realdir/$cmdname \$realdir/$cmdname.real $cmdoptions "\$@"
END
	chmod +x $cmd
}

create_merged_usr_symlinks() {
    root="$1"
    install -d $root/bin $root/sbin $root/lib
    lnr $root/bin $root/bin
    lnr $root/sbin $root/sbin
    lnr $root/lib $root/lib

    if [ "/lib" != "/lib" ]; then
       install -d $root/lib
       lnr $root/lib $root/lib
    fi

    # create base links for multilibs
    multi_libdirs=""
    for d in $multi_libdirs; do
        install -d $root/usr/$d
        lnr $root/usr/$d $root/$d
    done
}

create_merged_usr_symlinks_rootfs() {
    create_merged_usr_symlinks /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/rootfs
}

create_merged_usr_symlinks_sdk() {
    create_merged_usr_symlinks /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image/usr/local/oe-sdk-hardcoded-buildpath/sysroots/aarch64-poky-linux
}

create_sdk_files() {
	cp /home/parallels/yocto/poky/scripts/relocate_sdk.py /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/

	# Replace the ##DEFAULT_INSTALL_DIR## with the correct pattern.
	# Escape special characters like '+' and '.' in the SDKPATH
	escaped_sdkpath=$(echo /usr/local/oe-sdk-hardcoded-buildpath |sed -e "s:[\+\.]:\\\\\\\\\0:g")
	sed -i -e "s:##DEFAULT_INSTALL_DIR##:$escaped_sdkpath:" /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/sdk/image//usr/local/oe-sdk-hardcoded-buildpath/relocate_sdk.py
}

create_shar() {
	# copy in the template shar extractor script
	cp /home/parallels/yocto/poky/meta/files/toolchain-shar-extract.sh /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.sh

	rm -f /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/pre_install_command /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/post_install_command

	if [ "1" = "1" ] ; then
		cp /home/parallels/yocto/poky/meta/files/toolchain-shar-relocate.sh /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/post_install_command
	fi
	cat << "EOF" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/pre_install_command

EOF

	cat << "EOF" >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/post_install_command

EOF
	sed -i -e '/@SDK_PRE_INSTALL_COMMAND@/r /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/pre_install_command' \
		-e '/@SDK_POST_INSTALL_COMMAND@/r /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/temp/post_install_command' \
		/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.sh

	# substitute variables
	sed -i -e 's#@SDK_ARCH@#aarch64#g' \
		-e 's#@SDKPATH@#/usr/local/oe-sdk-hardcoded-buildpath#g' \
		-e 's#@SDKPATHINSTALL@#/opt/poky/3.1.30#g' \
		-e 's#@SDKEXTPATH@#~/poky_sdk#g' \
		-e 's#@OLDEST_KERNEL@#3.2.0#g' \
		-e 's#@REAL_MULTIMACH_TARGET_SYS@#aarch64-poky-linux#g' \
		-e 's#@SDK_TITLE@#Poky (Yocto Project Reference Distro) SDK#g' \
		-e 's#@SDK_VERSION@#3.1.30#g' \
		-e '/@SDK_PRE_INSTALL_COMMAND@/d' \
		-e '/@SDK_POST_INSTALL_COMMAND@/d' \
		-e 's#@SDK_GCC_VER@#None#g' \
		-e 's#@SDK_ARCHIVE_TYPE@#tar.xz#g' \
		/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.sh

	# add execution permission
	chmod +x /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.sh

	# append the SDK tarball
	cat /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.tar.xz >> /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.sh

	# delete the old tarball, we don't need it anymore
	rm /home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/aarch64-deploy-core-image-base-populate-sdk/poky-glibc-aarch64-core-image-base-aarch64-raspberrypi4-64-toolchain-3.1.30.tar.xz
}

create_wrapper() {
	# Create a wrapper script where extra environment variables are needed
	#
	# These are useful to work around relocation issues, by setting environment
	# variables which point to paths in the filesystem.
	#
	# Usage: create_wrapper FILENAME [[VAR=VALUE]..]

	cmd=$1
	shift

	echo "Generating wrapper script for $cmd"

	mv $cmd $cmd.real
	cmdname=`basename $cmd`
	dirname=`dirname $cmd`
	exportstring=$@
	if [ "" != "" ]; then
		relpath=`python3 -c "import os; print(os.path.relpath('/home/parallels/yocto/build/tmp/work/raspberrypi4_64-poky-linux/core-image-base/1.0-r0/image', '$dirname'))"`
		exportstring=`echo $@ | sed -e "s::\\$realdir/$relpath:g"`
	fi
	cat <<END >$cmd
#!/bin/bash
realpath=\`readlink -fn \$0\`
realdir=\`dirname \$realpath\`
export $exportstring
exec -a "\$0" \$realdir/$cmdname.real "\$@"
END
	chmod +x $cmd
}

die() {
	bbfatal_log "$*"
}

